{
  "extends": ["@antfu", "plugin:markdown/recommended"],
  "rules": {
    "no-tabs": ["error"]
  },
  "overrides": [
    {
      "files": ["**/*.md/**"],
      "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "@typescript-eslint/no-var-requires": "off"
      }
    }
  ]
}
